      "cell_type": "code",
      "source": [
        "!pip install -q tpot\n",
        "from tpot import TPOTRegressor\n",
        "\n",
        "tpot = TPOTRegressor(\n",
        "    generations=3,          # réduit pour Colab\n",
        "    population_size=10,\n",
        "    random_state=42,\n",
        "    n_jobs=1  # Changed to 1 to avoid distributed worker issues\n",
        ")\n",
        "tpot.fit(X_train, y_train)\n",
        "\n",
        "y_pred_tpot = tpot.predict(X_test)\n",
        "\n",
        "mse_tpot  = mean_squared_error(y_test, y_pred_tpot)\n",
        "rmse_tpot = np.sqrt(mse_tpot)\n",
        "mae_tpot  = mean_absolute_error(y_test, y_pred_tpot)\n",
        "r2_tpot   = r2_score(y_test, y_pred_tpot)\n",
